{
  "name": "lucodear-icons",
  "displayName": "lucodear-icons",
  "description": "üç≠ lucodear-icons ¬ª A PKief's 'Material Icons Theme' fork with my own custom additions. This was made for my personal use",
  "version": "0.17.0",
  "scripts": {
    "build": "npm run compile:dev && npm run generateJson",
    "check": "ts-node ./src/scripts/icons/checks",
    "compile": "webpack --config ./build/webpack.config.js --mode production",
    "compile:dev": "webpack --config ./build/webpack.config.js --mode none",
    "compile:watch": "webpack --config ./build/webpack.config.js --mode none --watch",
    "compile-web": "webpack --config ./build/web-extension.webpack.config.js",
    "watch-web": "webpack --watch --config ./build/web-extension.webpack.config.js",
    "package-web": "webpack --mode production --devtool hidden-source-map --config ./build/web-extension.webpack.config.js",
    "contributors": "ts-node ./src/scripts/contributors/index.ts",
    "generateJson": "ts-node ./src/scripts/icons/generateJson.ts",
    "lint": "eslint -c .eslintrc.json --ext .ts ./src/**/*.ts",
    "postcompile": "npm run generateJson && npm run check",
    "pretest": "npm run build && tsc -p ./",
    "preview": "ts-node ./src/scripts/preview && npm run preview-lc",
    "preview-lc": "ts-node ./src/scripts/preview/lucodear-preview",
    "preview-lc:dev": "ts-node ./src/scripts/preview/lucodear-preview --dev --size 16 && browser-sync start -c .\\build\\bs-config.js",
    "preview:dev:diffs": "ts-node ./src/scripts/preview/changes",
    "preview:dev:full-diff": "ts-node ./src/scripts/preview/full-diff",
    "preview:dev:full-diff:live": "ts-node ./src/scripts/preview/full-diff/live",
    "svgo": "svgo -i icons icons-lc -r -q",
    "test": "node ./out/test/runTest.js",
    "changelog": "changelog-machine --config changelog.config.json",
    "preversion": "npm run contributors && npm run preview && git add images/*.png",
    "version": "npm run changelog && git add CHANGELOG.md",
    "vscode:prepublish": "npm run lint && npm run compile && npm run package-web"
  },
  "publisher": "lucaslabs",
  "author": {
    "name": "Lucas Colombo",
    "email": "lucas@lucode.ar",
    "url": "https://lucode.ar"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "homepage": "https://github.com/lucas-labs/lucodear-icons/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucas-labs/lucodear-icons.git"
  },
  "bugs": {
    "url": "https://github.com/lucas-labs/lucodear-icons/issues"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#11131c",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "icons",
    "theme",
    "material",
    "icon-theme",
    "customization"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension",
  "browser": "./dist/web/extension.js",
  "sideEffects": false,
  "files": [
    "icons",
    "dist/material-icons.json"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "lucodear-icons",
        "label": "lucode.ar icons",
        "path": "./dist/material-icons.json",
        "_watch": true
      }
    ],
    "commands": [
      {
        "command": "lucodear-icons.activateIcons",
        "title": "%command.activateIcons%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.toggleIconPacks",
        "title": "%command.toggleIconPacks%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.changeFolderTheme",
        "title": "%command.changeFolderTheme%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.changeFolderColor",
        "title": "%command.changeFolderColor%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.changeFileColor",
        "title": "%command.changeFileColor%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.restoreDefaultConfig",
        "title": "%command.restoreDefaultConfig%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.toggleExplorerArrows",
        "title": "%command.toggleExplorerArrows%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.changeOpacity",
        "title": "%command.changeOpacity%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.toggleGrayscale",
        "title": "%command.toggleGrayscale%",
        "enablement": "!isWeb"
      },
      {
        "command": "lucodear-icons.changeSaturation",
        "title": "%command.changeSaturation%",
        "enablement": "!isWeb"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%configuration.title%",
      "properties": {
        "lucodear-icons.activeIconPack": {
          "type": "string",
          "default": "angular",
          "description": "%configuration.activeIconPack%",
          "enumDescriptions": [
            "%configuration.activeIconPack.angular%",
            "%configuration.activeIconPack.angular_ngrx%",
            "%configuration.activeIconPack.react%",
            "%configuration.activeIconPack.react_redux%",
            "%configuration.activeIconPack.qwik%",
            "%configuration.activeIconPack.vue%",
            "%configuration.activeIconPack.vue_vuex%",
            "%configuration.activeIconPack.nest%",
            "%configuration.activeIconPack.pest%",
            "%configuration.activeIconPack.none%"
          ],
          "enum": [
            "angular",
            "angular_ngrx",
            "react",
            "react_redux",
            "qwik",
            "vue",
            "vue_vuex",
            "nest",
            "pest",
            "none"
          ]
        },
        "lucodear-icons.files.associations": {
          "type": "object",
          "default": {},
          "description": "%configuration.files.associations%"
        },
        "lucodear-icons.folders.associations": {
          "type": "object",
          "default": {},
          "description": "%configuration.folders.associations%"
        },
        "lucodear-icons.languages.associations": {
          "type": "object",
          "default": {},
          "description": "%configuration.languages.associations%"
        },
        "lucodear-icons.folders.theme": {
          "type": "string",
          "default": "specific",
          "description": "%configuration.folders.theme%",
          "enumDescriptions": [
            "%configuration.folders.theme.specific%",
            "%configuration.folders.theme.classic%",
            "%configuration.folders.theme.none%"
          ],
          "enum": [
            "specific",
            "classic",
            "none"
          ]
        },
        "lucodear-icons.folders.color": {
          "type": "string",
          "default": "#90a4ae",
          "pattern": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
          "description": "%configuration.folders.color%"
        },
        "lucodear-icons.files.color": {
          "type": "string",
          "default": "#90a4ae",
          "pattern": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
          "description": "%configuration.files.color%"
        },
        "lucodear-icons.opacity": {
          "type": "number",
          "default": 1,
          "minimum": 0,
          "maximum": 1,
          "description": "%configuration.opacity%"
        },
        "lucodear-icons.hidesExplorerArrows": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.hidesExplorerArrows%"
        },
        "lucodear-icons.saturation": {
          "type": "number",
          "default": 1,
          "minimum": 0,
          "maximum": 1,
          "description": "%configuration.saturation%"
        },
        "lucodear-icons.lucodear.enable": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.lucodear.enable%"
        },
        "lucodear-icons.lucodear.ignoreLookupPaths": {
          "type": "array",
          "default": [
            "node_modules",
            ".git",
            ".venv",
            "venv",
            ".yarn"
          ],
          "description": "%configuration.lucodear.ignoreLookupPaths%"
        },
        "lucodear-icons.lucodear.files.regexAssociations": {
          "type": "object",
          "default": {},
          "description": "%configuration.lucodear.files.regexAssociations%"
        },
        "lucodear-icons.lucodear.folders.regexAssociations": {
          "type": "object",
          "default": {},
          "description": "%configuration.lucodear.folders.regexAssociations%"
        }
      }
    }
  },
  "dependencies": {
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.29.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.19.15",
    "@types/puppeteer": "^5.4.6",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.4.0",
    "browser-sync": "^3.0.2",
    "changelog-machine": "^1.0.2",
    "chokidar": "^3.6.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "lodash.debounce": "^4.0.8",
    "mocha": "^10.0.0",
    "prettier": "^3.2.5",
    "puppeteer": "^14.1.1",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.8.0",
    "typescript": "^5.3.3",
    "vscode-test": "^1.6.1",
    "webpack": "^5.71.1",
    "webpack-cli": "^5.1.4"
  }
}
